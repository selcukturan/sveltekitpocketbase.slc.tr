{
	"name": "sveltekitpocketbase.slc.tr",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint .",
		"slc:dev:push": "git add . && git commit -m \"SLC CLI Auto Push\" && git push origin main",
		"pb": "docker compose --file ./.slc-development/pocketbase/docker-compose.yml --env-file ./.env.development up -d --build",
		"pb:typegen": "pocketbase-typegen --db ./.slc-local/pocketbase/pb_data/data.db --out ./src/lib/client/types/pocketbase-types.ts",
		"slc:local:docker": "docker-compose -f docker-compose-local.yml --env-file ./.env.local up --build -d"
	},
	"devDependencies": {
		"@eslint/compat": "^1.2.9",
		"@eslint/js": "^9.26.0",
		"@sveltejs/adapter-node": "^5.2.12",
		"@sveltejs/kit": "^2.20.8",
		"@sveltejs/vite-plugin-svelte": "^5.0.3",
		"@tailwindcss/vite": "^4.1.7",
		"@types/node": "^22.15.17",
		"eslint": "^9.26.0",
		"eslint-config-prettier": "^10.1.5",
		"eslint-plugin-svelte": "^3.5.1",
		"globals": "^16.1.0",
		"pocketbase-typegen": "^1.3.1",
		"prettier": "^3.5.3",
		"prettier-plugin-svelte": "^3.3.3",
		"prettier-plugin-tailwindcss": "^0.6.11",
		"svelte": "^5.28.2",
		"svelte-check": "^4.1.7",
		"tailwindcss": "^4.1.7",
		"typescript": "^5.8.3",
		"typescript-eslint": "^8.32.0",
		"vite": "^6.3.5"
	},
	"dependencies": {
		"@zag-js/svelte": "^1.12.2",
		"@zag-js/toast": "^1.12.2",
		"@zag-js/tooltip": "^1.12.2",
		"bits-ui": "^1.4.8",
		"dotenv": "^16.5.0",
		"dotenv-expand": "^12.0.2",
		"pocketbase": "^0.26.0",
		"zod": "^3.24.4",
		"zod-form-data": "^2.0.7"
	}
}
