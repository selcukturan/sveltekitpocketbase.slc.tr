{
	"name": "sveltekitpocketbase.slc.tr",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint .",
		"slc:dev:push": "git add . && git commit -m \"SLC CLI Auto Push\" && git push origin main",
		"pb": "docker compose --file ./.slc-development/pocketbase/docker-compose.yml --env-file ./.env.development up -d --build",
		"pb:typegen": "pocketbase-typegen --db ./.slc-production/pocketbase/pb_data/data.db --out ./src/lib/client/types/pocketbase-types.ts",
		"slc:production:docker": "docker-compose -f docker-compose-local.yml --env-file ./.env.local up --build -d"
	},
	"devDependencies": {
		"@eslint/compat": "^1.3.1",
		"@eslint/js": "^9.32.0",
		"@sveltejs/adapter-node": "^5.2.13",
		"@sveltejs/kit": "^2.27.0",
		"@sveltejs/vite-plugin-svelte": "^6.1.0",
		"@tailwindcss/forms": "^0.5.10",
		"@tailwindcss/vite": "^4.1.11",
		"@types/node": "^22.15.33",
		"eslint": "^9.32.0",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-svelte": "^3.11.0",
		"globals": "^16.3.0",
		"pocketbase-typegen": "^1.3.1",
		"prettier": "^3.6.2",
		"prettier-plugin-svelte": "^3.4.0",
		"prettier-plugin-tailwindcss": "^0.6.14",
		"svelte": "^5.37.3",
		"svelte-check": "^4.3.1",
		"tailwindcss": "^4.1.11",
		"typescript": "^5.8.3",
		"typescript-eslint": "^8.38.0",
		"vite": "^7.0.6"
	},
	"dependencies": {
		"dotenv": "^17.2.1",
		"dotenv-expand": "^12.0.2",
		"pocketbase": "^0.26.2",
		"sveltekit-superforms": "^2.27.1",
		"valibot": "^1.1.0"
	}
}
