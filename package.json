{
	"name": "sveltekitpocketbase.slc.tr",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint .",
		"slc:dev:push": "git add . && git commit -m \"SLC CLI Auto Push\" && git push origin main",
		"pb": "docker compose --file ./.slc-development/pocketbase/docker-compose.yml --env-file ./.env.development up -d --build",
		"slc:local:docker": "docker-compose -f docker-compose-local.yml --env-file ./.env.local up --build -d"
	},
	"devDependencies": {
		"@eslint/compat": "^1.2.8",
		"@eslint/js": "^9.24.0",
		"@sveltejs/adapter-node": "^5.2.12",
		"@sveltejs/kit": "^2.20.5",
		"@sveltejs/vite-plugin-svelte": "^5.0.3",
		"@tailwindcss/vite": "^4.1.3",
		"@types/node": "^22.14.1",
		"eslint": "^9.24.0",
		"eslint-config-prettier": "^10.1.2",
		"eslint-plugin-svelte": "^3.5.1",
		"globals": "^16.0.0",
		"prettier": "^3.5.3",
		"prettier-plugin-svelte": "^3.3.3",
		"prettier-plugin-tailwindcss": "^0.6.11",
		"svelte": "^5.26.2",
		"svelte-check": "^4.1.6",
		"tailwindcss": "^4.1.3",
		"tailwindcss-animate": "^1.0.7",
		"typescript": "^5.8.3",
		"typescript-eslint": "^8.29.1",
		"vite": "^6.2.6"
	},
	"dependencies": {
		"@internationalized/date": "^3.8.0",
		"bits-ui": "^1.3.19",
		"dotenv": "^16.5.0",
		"dotenv-expand": "^12.0.1",
		"pocketbase": "^0.25.2",
		"zod": "^3.24.2"
	}
}
